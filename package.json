{
  "name": "@magic-mcp/monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "AI-Native MCP Platform for Production - Generate secure, production-ready MCP servers in minutes",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "anthropic",
    "code-generation",
    "openapi",
    "api",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magic-mcp/magic-mcp.git"
  },
  "license": "MIT",
  "author": "Magic MCP Team",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "turbo": "^1.12.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@10.2.4"
}
